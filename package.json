    "eslint": "^6.5.0",
    "eslint-plugin-cypress": "^2.7.0",
    "rollup": "^1.19.4",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-uglify": "^6.0.2"
